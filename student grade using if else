#include<stdio.h>
main()
{
    int a;
    printf("enter a student maqrks=");
    scanf("%d",&a);

    if(a<=100 && a>=85)
    {
        printf("congo u got GRADE A");
    }
    else if(a<85 && a>=70)
    {
        printf("congo u got GRADE B");
    }
      else if(a<=69 && a>=55)
    {
        printf("congo u got GRADE C");
    }
      else if(a<=54 && a>=40)
    {
        printf("got GRADE D");
    }
      else if(a<=40)
    {
        printf("BATTER LUCK NEXT TIME  GRADE F");
    }




}
